{
  "version": 2,
  "builds": [
    {
      "src": "frontend/dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/frontend/dist/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(manifest\\.webmanifest|registerSW\\.js|sw\\.js|vite\\.svg|workbox-.*\\.js)",
      "dest": "/frontend/dist/$1"
    },
    {
      "src": "/pr/(.*)",
      "dest": "/frontend/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}
